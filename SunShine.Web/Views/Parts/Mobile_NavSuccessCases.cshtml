@using SunShine.Model;
@model List<SiteCategoryViewModel>
@{
    Layout = null;
}
@{
    if (Model != null && Model.Count > 0)
    {
        <div class="mobile_success_case_box">
            @for (int i = 0; i < Model.Count; i++)
            {
              <div class="mobile_success_case_item"><a href="@Url.Action("Index","Case",new { categoryCode=Request["categoryCode"],currentCategoryCode= Model[i].categorycode })">@Model[i].categoryname</a></div>
            }
        </div>
        <div style="clear:both;"></div>
    }
}
